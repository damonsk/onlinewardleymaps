{
    "name": "wmlandscape",
    "version": "3.0.1",
    "description": "The engine that powers OnlineWardleyMaps and VSCode-Wardley-Maps",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "rollup -c && cp ./src/index.d.ts ./dist/ && cp ./src/global.d.ts ./dist/",
        "start": "rollup -c -w",
        "types": "tsc --skipLibCheck -p tsconfig.types.json || true",
        "pack:check": "npm pack --dry-run --cache /tmp/npm-cache",
        "prepack": "npm run build",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist"
    ],
    "keywords": [],
    "author": "Damon Skelhorn",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/damonsk/onlinewardleymaps.git",
        "directory": "frontend/wmlandscape"
    },
    "bugs": {
        "url": "https://github.com/damonsk/onlinewardleymaps/issues"
    },
    "homepage": "https://github.com/damonsk/onlinewardleymaps/tree/main/frontend/wmlandscape",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.24.3",
        "@babel/preset-env": "^7.24.3",
        "@babel/preset-react": "^7.24.1",
        "@babel/preset-typescript": "^7.24.1",
        "@rollup/plugin-babel": "^5.2.1",
        "@rollup/plugin-commonjs": "^15.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-typescript": "^8.3.2",
        "@types/lodash.merge": "^4.6.9",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "autoprefixer": "^10.0.1",
        "babel-core": "^6.26.3",
        "babel-runtime": "^6.26.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "@mui/material": "^7.1.2",
        "@mui/icons-material": "^7.1.2",
        "@mui/styles": "^6.4.12",
        "lodash.merge": "^4.6.2",
        "i18next": "^23.16.8",
        "next": "^15.3.4",
        "rollup": "^4.13.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
