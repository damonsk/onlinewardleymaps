{
	"name": "online-wardley-maps",
	"version": "1.117.0",
	"main": "index.js",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "7.26.0",
		"@babel/eslint-parser": "^7.25.9",
		"@babel/preset-env": "7.26.0",
		"@babel/preset-react": "7.25.9",
		"@next/eslint-plugin-next": "14.2.16",
		"@testing-library/jest-dom": "^6.1.6",
		"@types/node": "20.10.5",
		"@types/react": "18.2.45",
		"babel-plugin-require-context-hook": "^1.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jsx": "^0.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-react": "^7.33.2",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1",
		"pretty-quick": "^3.1.3",
		"rollup": "^4.9.1",
		"typescript": "5.6.3"
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jest": true
	},
	"build": {
		"directories": {
			"output": "release/"
		}
	},
	"resolutions": {
		"//": "See https://github.com/facebook/create-react-app/issues/11773",
		"react-error-overlay": "6.0.9"
	},
	"dependencies": {
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.15.2",
		"@mui/material": "5.15.2",
		"@mui/styles": "5.15.2",
		"ace-builds": "^1.32.2",
		"core-js": "^3.34.0",
		"html2canvas": "^1.4.1",
		"lodash.merge": "^4.6.2",
		"next": "14.0.4",
		"prop-types": "^15.8.1",
		"react": "18.2.0",
		"react-ace": "^10.1.0",
		"react-dom": "18.2.0",
		"react-svg-pan-zoom": "^3.13.1",
		"sass": "^1.69.5",
		"styled-components": "6.1.5"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"yarn run format-staged",
			"yarn run elint frontend/ --fix",
			"git add"
		]
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint .",
		"format-staged": "prettier --write \"$(git diff --name-only --cached)\"",
		"precommit": "yarn run format-staged && yarn test",
		"package": "rollup -c",
		"test": "jest --env=jsdom",
		"test:watch": "jest --watchAll --env=jsdom"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
